{
    "name": "nearata/lang-italian",
    "type": "flarum-extension",
    "description": "Italian language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.8"
    },
	"authors": [
        {
            "name": "William Di Cicco",
            "email": "nearata@protonmail.com"
        }
    ],
	"support": {
        "issues": "https://github.com/Nearata/flarum-ext-italian/issues",
        "source": "https://github.com/Nearata/flarum-ext-italian"
    },
    "extra": {
        "flarum-extension": {
            "title": "Italian",
